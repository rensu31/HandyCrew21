<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="HandyCrew.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:HandyCrew"
    xmlns:views="using:HandyCrew.Views"
    Title="HandyCrew"
    Shell.FlyoutBehavior="Disabled">

    <TabBar>
        <ShellContent
            Title="Students"
            ContentTemplate="{DataTemplate views:UserDashboard}"
            Icon="pr" />
        <ShellContent
            Title="Course"
            ContentTemplate="{DataTemplate views:Profile}"
            Icon="cr" />


    </TabBar>

</Shell>
